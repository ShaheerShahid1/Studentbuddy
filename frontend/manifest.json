{
  "manifest_version": 3,
  "name": "Student Buddy",
  "version": "1.0",
  "description": "Helps students gather and organize online resources for assignments.",
  "permissions": ["storage", "tabs", "downloads", "contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
